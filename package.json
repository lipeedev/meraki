{
  "name": "meraki-lang",
  "version": "0.1.0",
  "description": "a programming language made for educational purposes",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "npx tsx src/index.ts",
    "build": "npx tsc",
    "lint": "npx eslint --fix src/**/*.ts",
    "prepare": "npx husky install",
    "commit": "git add . && npx git-cz"
  },
  "bin": {
    "meraki": "dist/index.js"
  },
  "keywords": [
    "programming-language",
    "meraki",
    "meraki-lang"
  ],
  "author": "lipeedev",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/lipeedev/meraki.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@eslint/create-config": "0.4.6",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "tsx": "^3.14.0"
  }
}
